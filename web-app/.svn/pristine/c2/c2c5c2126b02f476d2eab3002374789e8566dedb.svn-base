/* 图层控制样式 */
/* 右下角菜单 */
.mapType-wrapper .mapType .clk  span.active{
    background-color: #89BED9;
}
.mapType-wrapper label {
    display: inline-block;
}

.mapType-wrapper {
    font-size: 14px;
    position: absolute;
    bottom: 20px;
    right: 0;
    z-index:10;
}
.mapType {
    height: 76px;
    cursor: pointer;
    -webkit-transition-property: width, background-color;
    -ms-transition-property: width, background-color;
    transition-property: width, background-color;
    -webkit-transition-duration: .4s;
    -ms-transition-duration: .4s;
    transition-duration: .4s;
    width: 110px;
    -webkit-transition-duration: .4s;
    -ms-transition-duration: .4s;
    transition-delay: 0s;
}
.mapType .mapTypeCard span {
    background-color: #000;
    background-color: rgba(0,0,0,0.5);
}
.mapType-wrapper:hover .mapType .mapTypeCard:hover span {
    background-color: #89BEDA;
}
.mapType-wrapper:hover .mapType .mapTypeCard.active,
.mapType .mapTypeCard.active {
     border: 2px solid #337fe5
}
.mapType-wrapper .mapType .second {
    background-image: url("../images/layer/yingxiang.jpg");
}
.mapType-wrapper .mapType .ldxb {
    background-image: url("../images/layer/ldxb.jpg");
}
.mapType-wrapper .mapType .dcfw {
    background-image: url("../images/layer/dcfw.png");
}
.mapType-wrapper .mapType .ygyx {
    background-image: url("../images/layer/ygyx.jpg");
}
.mapType .mapTypeCard {
    height: 60px;
    width: 86px;
    position: absolute;
    border-radius: 2px;
    top: 8px;
    box-sizing: border-box;
    border: 1px solid transparent;
    background-color:#000;
    background-color: rgba(0,0,0,.2);
    -webkit-transition-property: right, background-image;
    -ms-transition-property: right, background-image;
    transition-property: right, background-image;
    -webkit-transition-duration: .4s;
    -ms-transition-duration: .4s;
    transition-duration: .4s;
    -webkit-transition-delay: 0s;
    -ms-transition-delay: 0s;
    transition-delay: 0s;
}
.mapType .mapTypeCard span {
    position: absolute;
    bottom: -1px;
    left: -1px;
    right: -1px;
    display: inline-block;
    padding: 3px 0px 2px 0px;
    font-size: 12px;
    height: 12px;
    line-height: 12px;
    color: #fff;
    height: 16px;
    text-align: center;
    border-bottom-left-radius: 2px;
    border-bottom-right-radius: 2px;
}
.mapType .mapTypeCard.active span,
.mapType .mapTypeCard:hover span {
    background-color: #89BEDA;
}
.mapType .mapTypeCard:hover .sub-menu {
    display: block;
}
.mapType .mapTypeCard:hover {
     border: 2px solid #337fe5;
}
.mapType .mapTypeCard:hover .switch-box {
    visibility: visible
}
.mapType .second:hover .switch-box {
    visibility: visible
}
.mapType .first {
    z-index: 10;
    right: 10px;
    background-image: url('../images/layer/03.png');
}
.mapType .second {
    z-index: 9;
    right: 15px;
    background-color: #fff;
    background-color: rgba(0,0,0,.2);
}
.mapType .card{
    z-index: 8;
    right: 10px;
}

.mapType .switch-box {
    visibility: hidden;
    position: absolute;
    width: 100%;
    height: 20px;
    line-height: 22px;
    color: #fff;
    text-align: center;
    cursor: pointer;
    background-color: #fff;
    background: rgba(0, 0, 0, .5)
}
.mapType .switch-box p {
    display: inline-block;
}
.mapType .switch-box input.switch {
    display: inline-block;
    vertical-align: middle;
    cursor: pointer
}
.mapType .sub-menu {
    display: none;
    width: auto;
    position: absolute;
    left: 50%;
    bottom: 57px;
    transform: translateX(-50%);
    -ms-transform: translateX(-50%);    /* IE 9 */
    -moz-transform: translateX(-50%);   /* Firefox */
    -webkit-transform: translateX(-50%); /* Safari 和 Chrome */
    -o-transform: translateX(-50%);
}
.mapType .sub-menu label{
    cursor: pointer;
    width: 100%;
}
.mapType .sub-menu .trigon {
  width: 0;
  height: 0;
  border-width: 10px;
  border-bottom-width: 0;
  border-style: solid;
  margin: 0 auto;
  border-color: #fff transparent transparent;
}
.mapType .sub-menu ul {
    background-color: #fff;
    padding: 8px;
    border-radius: 5px;
}
.mapType .sub-menu li {
    list-style: none;
    line-height: 24px;
}
.mapType .sub-menu li input {
    display: inline-block;
    vertical-align: middle;
}
.mapType .sub-menu li span {
    position: relative;
    width: 30px;
    height: 15px;
    padding: 0;
    vertical-align: middle;
    margin: 0 4px;
}
.mapType .sub-menu li {
    white-space: nowrap;
}
