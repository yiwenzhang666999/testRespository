/**
*退出按钮样式
*zyw 2018.03.02
*/
.zs-zyjg-header .user .name .fore-sys-user-exit {
    position: absolute;
    top: -1px;
    bottom: -5px;
    right: -22px;
    width: 24px;
    background: url(../images/fore-sys-user-control-exit.png) no-repeat center;
}
/**
*用户名样式
*zyw 2018.03.02
*/
.zs-zyjg-header .user .name {
    position: relative;
    display: inline-block;
    height: 38px;
    line-height: 40px;
    color: #fff;
    left: -10px;
}
/**
*用户名图标样式
*zyw 2018.03.02
*/
.zs-zyjg-header .user .name .icon {
    position: absolute;
    top: 0;
    bottom: 0;
    left: -23px;
    width: 24px;
    background: url(../images/zs-user-icon.png) no-repeat center;
}