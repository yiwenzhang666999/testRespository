package com.forestar.cache.redis;

public class SpendTimeUtil {

	long startTime = -1;
	
	long endTime = -1;
	
	public SpendTimeUtil(){
		startTime = System.currentTimeMillis();
	}
	
	public void printSpendTime(){
		endTime = System.currentTimeMillis();
		System.out.println("花费---------->"+(endTime - startTime) + "ms");
	}
}
