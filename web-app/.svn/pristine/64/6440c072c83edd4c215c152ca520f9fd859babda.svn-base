* {
	box-sizing: border-box;
}

body {
	background: #f1f1f1;
}
/*common/全局*/
.inner {
	position: relative;
	width: 1160px;
	margin: 0 auto;
	clear: both;
}
/*头部*/
.zs-sys-header {
	height: 180px;
	background: url(../images/zs-header-bg.png) no-repeat center;
}

.zs-sys-header .inner {
	height: 100%;
}

.zs-sys-header .logo {
	width: 100%;
	height: 100%;
	background: url(../images/zs-logo.png) no-repeat 35px center;
}

.zs-sys-header .config {
	position: absolute;
	top: 30px;
	right: 0px;
}

.zs-sys-header .config input {
	width: 80px;
	height: 32px;
	line-height: 32px;
	color: #fff;
	border: none;
	outline: none;
	cursor: pointer;
}

.zs-sys-header .config .login-btn {
	margin-right: 12px;
	background: url(../images/zs-login-btn-bg.png) no-repeat center;
}

.zs-sys-header .config .reg-btn {
	background: url(../images/zs-reg-btn-bg.png) no-repeat center;
}
/*menu*/
.zs-sys-menu {
	height: 46px;
	background: #61ba37 url(../images/zs-menu-bg.png) no-repeat center;
}

.zs-sys-menu .menu {
	height: 46px;
}

.zs-sys-menu .menu .list {
	font-size: 0px;
}

.zs-sys-menu .menu .list a {
	position: relative;
	display: inline-block;
	vertical-align: top;
	width: 135px;
	height: 46px;
	line-height: 46px;
	font-size: 16px;
	text-align: center;
	color: #fff;
	border-right: 1px #55b315 solid;
	z-index: 10;
}

.zs-sys-menu .menu .list a:after {
	display: block;
	content: "";
	position: absolute;
	top: 0px;
	bottom: 0px;
	right: 0px;
	width: 1px;
	background: #349508;
}

.zs-sys-menu .menu .list a.selected:after {
	display: none;
}

.zs-sys-menu .menu .list i.slect-mask {
	position: absolute;
	top: -4px;
	left: -5px;
	bottom: 0px;
	width: 145px;
	background: url(../images/zs-menu-selected.png) no-repeat center;
	z-index: 1;
	display: none;
}

.zs-sys-menu .menu .list a.sy.selected~i.slect-mask {
	left: -5px;
	display: block;
}

.zs-sys-menu .menu .list a.gzgl.selected~i.slect-mask {
	left: 130px;
	display: block;
}

.zs-sys-menu .menu .list a.zttj.selected~i.slect-mask {
	left: 265px;
	display: block;
}

.zs-sys-menu .menu .list a.yysc.selected~i.slect-mask {
	left: 400px;
	display: block;
}

.zs-sys-menu .menu .list a.jsjl.selected~i.slect-mask {
	left: 535px;
	display: block;
}

.zs-sys-menu .menu .list a.jswd.selected~i.slect-mask {
	left: 670px;
	display: block;
}

.zs-sys-menu .menu .search {
	position: absolute;
	top: 0px;
	bottom: 0px;
	right: 25px;
	line-height: 42px;
	font-size: 16px;
}

.zs-sys-menu .menu .search .menu-btn {
	width: 57px;
	height: 28px;
	line-height: 28px;
	background: rgba(255,255,255,0.8);
	border: none;
	outline: none;
	color: #2f7700;
	cursor: pointer;
	transition: all 0.3s;
}

.zs-sys-menu .menu .search .menu-btn:hover {
	background: rgba(255,255,255,0.6);
}

.zs-sys-menu .menu .search .menu-input {
	width: 235px;
	height: 28px;
	padding: 0px 5px;
	line-height: 28px;
	background: rgba(0,0,0,0.3);
	font-size: 14px;
	border: none;
	outline: none;
	color: #fff;
}
/*body common*/
.zs-card {
	padding: 10px 0px;
	font-size: 0px;
}

.zs-card .item {
	position: relative;
	display: inline-block;
	vertical-align: top;
	font-size: 14px;
	background: #fff;
	box-shadow: 0px 0px 10px #ccc;
	-moz-box-shadow: 0px 0px 10px #ccc;
	-webkit-box-shadow: 0px 0px 10px #ccc;
	overflow: hidden;
}

.zs-card .item .content {
	position: relative;
	height: -webkit-calc(100% - 42px);
	height: -moz-calc(100% - 42px);
	height: calc(100% - 42px);
	padding: 20px;
	overflow: hidden;
}
	/*title*/
.zs-card .item .title {
	position: relative;
	height: 42px;
	line-height: 42px;
	border-right: 1px #def2da solid;
	background: url(../images/zs-title.png) repeat-x center;
}

.zs-card .item .title h2 {
	height: 42px;
	padding-left: 70px;
	font-size: 16px;
	font-weight: normal;
	color: #fff;
}

.zs-card .item .title h2.bg-1 {
	background: url(../images/zs-title-bg1.png) no-repeat left center;
}

.zs-card .item .title h2.bg-2 {
	background: url(../images/zs-title-bg2.png) no-repeat left center;
}

.zs-card .item .title h2.bg-3 {
	background: url(../images/zs-title-bg3.png) no-repeat left center;
}

.zs-card .item .title h2.bg-4 {
	background: url(../images/zs-title-bg4.png) no-repeat left center;
}

.zs-card .item .title h2.bg-5 {
	background: url(../images/zs-title-bg5.png) no-repeat left center;
}

.zs-card .item .title .more {
	position: absolute;
	top: 0px;
	right: 20px;
	bottom: 0px;
	font-size: 14px;
	color: #999;
}

.zs-card .item .title .more:hover {
	color: #ff6600;
}
	/*列表*/
.zs-txt-list {
	height: 100%;
	overflow: hidden;
}

.zs-txt-list a {
	position: relative;
	display: block;
	height: 48px;
	line-height: 48px;
	border-bottom: 1px #ddd dashed;
	color: #666;
	font-size: 0;
}

.zs-txt-list a:hover {
	color: #ff6600;
}

.zs-txt-list a .txt {
	vertical-align: top;
	display: inline-block;
	width: 90%;
	padding-left: 10px;
	white-space: nowrap;
	text-overflow: ellipsis;
	font-size: 14px;
	overflow: hidden;
}

.zs-txt-list.with-time a .txt {
	width: 60%;
}

.zs-txt-list.with-circle a .txt {
	padding-left: 25px
}

.zs-txt-list.with-time a .time {
	vertical-align: top;
	display: inline-block;
	width: 40%;
	font-size: 14px;
	text-align: right;
}

.zs-txt-list.with-circle a:before {
	display: block;
	content: "";
	position: absolute;
	top: 48%;
	left: 10px;
	width: 3px;
	height: 3px;
	background: #ccc;
}
/*body common--end*/	
/*banner*/
.zs-card .item.banner {
	width: 690px;
	height: 435px;
	margin: 0px 10px 10px 0px;
}
/*图片*/
.zs-card .banner .list {
	height: 100%;
}

.zs-card .banner .list li {
	float: left;
	width: 690px;
	height: 435px;
	text-align: center;
	line-height: 435px;
}

.zs-card .banner .list li a{
	display: block;
}

.zs-card .banner .list li img {
	display: block;
	width: 100%;
	height: 100%;
	padding: 0;
	border: none;
}
/*按钮*/
.zs-card .banner .btns {
	position: absolute;
	right: 25px;
	bottom: 15px;
	font-size: 0;
}

.zs-card .banner .btns i {
	display: inline-block;
	width: 8px;
	height: 8px;
	margin-left: 5px;
	border-radius: 100%;
	background: #919fa7;
	cursor: pointer;
}

.zs-card .banner .btns i.selected {
	background: #2f7700;
}
/*banner--end*/
/*动态资讯*/
.zs-card .dtzx {
	width: 460px;
	height: 435px;
	margin-bottom: 10px;
}

.zs-card .dtzx .content {
	padding: 20px 20px 35px 20px;
}
/*动态资讯--end*/
/*林地变更工作进度情况*/
.zs-card .gzjd {
	width: 100%;
	height: 550px;
	padding: 0;
	margin-bottom: 10px;
}

.zs-card .gzjd .title h2 {
	padding-left: 32px;
}
	/*地图*/
.zs-card .gzjd .map {
	width: 760px;
	height: 100%;
	overflow: hidden;
}

.zs-card .gzjd .map img {
	max-width: 100%;
	max-height: 100%;
}

.zs-card .gzjd .table {
	position: absolute;
	top: 10px;
	bottom: 10px;
	left: 780px;
	width: 326px;
	padding: 9px 20px;
	box-shadow: 0px 0px 20px #ddd;
	-moz-box-shadow: 0px 0px 20px #ddd;
	-webkit-box-shadow: 0px 0px 20px #ddd;
}

.zs-card .gzjd .table table {
	border-collapse: collapse;
}

.zs-card .gzjd .table table thead tr {
	border: 1px #e1eedf solid;
}

.zs-card .gzjd .table table th {
	height: 35px;
	font-weight: normal;
	color: #5bb533;
	background: #f6faf5;
	text-align: center;
}

.zs-card .gzjd .table table td {
	height: 31px;
	text-align: center;
	color: #666;
}

.zs-card .gzjd .table table tbody tr:nth-child(even) {
	color: #666;
	background: #f4f4f4;
}
/*林地变更工作进度情况--end*/
.zs-card .jswd,.zs-card .yysc,.zs-card .hdjl {
	width: 380px;
	height: 370px;
	margin-right: 10px;
}
/*技术文档*/
.zs-card .jswd {
}
/*技术文档--end*/
/*应用市场*/
.zs-goods {
	height: 100%;
	overflow: hidden;
}

.zs-goods .good {
	height: 144px;
	padding: 0px 0px 20px 0px;
	border-bottom: 1px #ddd dashed;
	font-size: 0px;
	overflow: hidden;
}

.zs-goods .good:nth-child(even) {
	padding: 20px 0px 0px 0px;
	border: none;
}

.zs-goods .context ,.zs-goods .img {
	display: inline-block;
	height: 100%;
	vertical-align: top;
}

.zs-goods .context {
	width: 60%;
}

.zs-goods .img {
	width: 40%;
	line-height: 128px;
	text-align: center;
	overflow: hidden;
}

.zs-goods .img img {
	max-width: 100%;
	max-height: 100%;
	vertical-align: middle;
}

.zs-goods .context .name {
	margin-bottom: 14px;
	color: #3b8e05;
	font-size: 14px;
}

.zs-goods .context .desc {
	height: 52px;
	margin-bottom: 10px;
	font-size: 14px;
	color: #666;
	line-height: 26px;
	overflow: hidden;
	text-overflow: clip;
}

.zs-goods .context .price {
	font-size: 16px;
	font-weight: bold;
	color: #ff6709;
}
/*应用市场--end*/
/*互动交流*/
.zs-card .hdjl {
	margin-right: 0px;
}
/*互动交流--end*/
/*footer*/
.zs-sys-footer {
	font-size: 12px;
	text-align: center;
	color: #666;
}

.zs-sys-footer .inner {
	padding: 30px 0px;
	background: #fff;
}

.zs-sys-footer p {
	margin-bottom: 15px;
}

.zs-sys-footer p span {
	margin: 0px 12px;
}

.zs-sys-footer p.mar1 span {
	margin: 0px 6px;
}
/*footer--end*/

/*工作文档页面*/
.zs-card .item.full{
	width: 100%;
}

.zs-card .item.zcfg {
	margin-bottom: 10px;
}

/*工具与应用页面*/
.zs-sys-body .inner.pad {
	padding: 10px 0;
}

.zs-sys-body .zs-shop {
	padding: 20px 22px;
	background: #fff;
}
	/*筛选*/
.zs-filter {
	margin-bottom: 25px;
}

.zs-filter h2.title {
	/*margin-top: 15px;*/
	/*background: #f1f1f1;*/
	margin-bottom: 5px;
	height: 30px;
	line-height: 30px;
	font-size: 16px;
	color: #5bb533;
}

.zs-filter .item {
	position: relative;
	border-bottom: 1px #e4e4e4 dashed;
	line-height: 37px;
}

.zs-filter .item:nth-last-child(1) {
	border-bottom: 1px #e4e4e4 solid;
}

.zs-filter .item .data-title {
	display: table-cell;
	width: 70px;
	text-align: right;
	color: #666;
}

.zs-filter .item .data-content {
	display: table-cell;
	padding: 0px 60px 0px 20px;
	height: 37px;
	color: #333;
}

.zs-filter .item .data-content a {
	display: inline-block;
	height: 24px;
	padding: 0px 12px;
	line-height: 22px;
	border: 1px transparent solid;
	margin-right: 5px;
}

.zs-filter .item .data-content a.selected {
	border-color: #3b9513;
	background: #5bb533;
	color: #fff;
}

.zs-filter .item a.more {
	position: absolute;
	display: none;
	top: 8px;
	right: 0px;
	width: 45px;
	height: 22px;
	padding: 0;
	margin: 0;
	border: none;
	line-height: 22px;
	text-align: center;
	background: #efefef;
	color: #666;
}
	/*更多选项*/
.zs-filter .more-options {
	position: relative;
	height: 25px;
}

.zs-filter .more-options a {
	position: absolute;
	display: block;
	top: -1px;
	left: 50%;
	margin-left: -45px;
	width: 90px;
	height: 25px;
	border: 1px #e4e4e4 solid;
	border-top: none;
	text-align: center;
	line-height: 24px;
	background: #fff;
}
	/*列表*/
.zs-list h4.title {
	margin-top: 15px;
	font-size: 14px;
	line-height: 24px;
	font-weight: normal;
}

.zs-list .item p.price {
	position: absolute;
	bottom: 15px;
	left: 10px;
	font-size: 14px;
	font-weight: bold;
	color: #ff6709;
}

.zs-list p.desc {
	margin-bottom: 12px;
	font-size: 12px;
	color: #666;
	line-height: 22px;
}

.zs-list .list-box {
	width: 1220px;
	font-size: 0px;
}

.zs-list .list-box .item {
	position: relative;
	display: inline-block;
	width: 218px;
	height: 300px;
	margin: 0px 6px 20px 0px;
	border: 1px #e6e6e6 solid;
	font-size: 14px;
	background: #fff;
	transition: all 0.3s;
}

.zs-list .list-box .item:hover h4 {
	color: #ff6600;
}

.zs-list .list-box .item:hover {
	z-index: 99;
	transform: translateY(-5px);
	background: #fff;
	-moz-box-shadow: 0px 0px 20px rgba(0,0,0,.2);
	-webkit-box-shadow: 0px 0px 20px rgba(0,0,0,.2);
	box-shadow: 0px 0px 20px rgba(0,0,0,.2);
}

.zs-list .list-box .item a {
}

.zs-list .list-box .item .img {
	height: 170px;
	line-height: 170px;
	text-align: center;
	background: #f7f7f7;
	overflow: hidden;
}

.zs-list .list-box .item .img img {
	vertical-align: middle;
}

.zs-list .list-box .item .context {
	padding: 10px;
}

.zs-list .list-box .item .details {
	position: absolute;
	bottom: 10px;
	right: 10px;
	display: block;
	width: 80px;
	height: 28px;
	line-height: 28px;
	text-indent: 22px;
	background: #f1f1f1;
	font-size: 12px;
}

.zs-list .list-box .item .details i.info-icon {
	position: absolute;
	top: 0px;
	left: 2px;
	width: 22px;
	height: 28px;
	background: url(../images/zs-info.png) no-repeat center;
}

.zs-list .list-box .item .details:hover {
	background: #5bb533;
	color: #fff;
}

.zs-list .list-box .item .details:hover i.info-icon {
	background: url(../images/zs-info-s.png) no-repeat center;
}

/*分页*/
.zs-pagination {
	font-size: 12px;
	padding-top: 20px;
	margin-bottom: 50px;
	user-select: none;
	text-align: center;
}

.zs-pagination:after {
	display: block;
	visibility: hidden;
	clear: both;
	overflow: hidden;
	height: 0;
	content: ' ';
}

.zs-pagination .item {
	font-family: Arial;
	line-height: 28px;
	display: inline-block;
	width: 28px;
	height: 28px;
	margin-right: 8px;
	list-style: none;
	cursor: pointer;
	text-align: center;
	border: 1px solid #d9d9d9;
	border-radius: 6px;
	color: #666;
	box-shadow: none;
}

.zs-pagination .item.middle-page {
	border: none;
}

.zs-pagination .item.middle-page:hover {
	color: #666;
}

.zs-pagination .item a {
	margin: 0 6px;
	transition: none;
	text-decoration: none;
	color: #666;
}

.zs-pagination .item:hover {
	transition: all .3s ease;
	border-color: #327860;
	color: #327860;
}

.zs-pagination .item:hover a {
	color: #4bbe51;
}

.zs-pagination .item.selected {
	border-color: transparent;
	background-color: #5bb533;
}

.zs-pagination .item.selected a,
.zs-pagination .item.selected:hover a {
	color: #fff;
}

.zs-pagination .jump-next {
	line-height: 28px;
	display: inline-block;
	min-width: 28px;
	height: 28px;
	margin-right: 8px;
	cursor: pointer;
	text-align: center;
}

.zs-pagination .prev {
	margin-right: 8px;
}

.zs-pagination .prev,.zs-pagination .next {
	font-family: Arial;
	line-height: 28px;
	display: inline-block;
	display: inline-block;
	min-width: 28px;
	height: 28px;
	line-height: 28px;
	list-style: none;
	cursor: pointer;
	text-align: center;
	color: #666;
	border: 1px solid #d9d9d9;
	border-radius: 6px;
	vertical-align: top;
}

.zs-pagination .prev:hover,
.zs-pagination .next:hover {
	border-color: #4bbe51;
}

.zs-pagination .prev a {
	display: inline-block;
	width: 5px;
	height: 9px;
	margin-top: -.5px;
	background: url(../images/zs-page-prev.png) no-repeat center;
}

.zs-pagination .next a {
	display: inline-block;
	width: 5px;
	height: 9px;
	background: url(../images/zs-page-next.png) no-repeat center;
}

.zs-pagination .options {
	display: inline-block;
	margin-left: 15px;
	vertical-align: middle;
}

.zs-pagination .options .zs-select {
	height: 28px;
	line-height: 28px;
	width: 80px;
	border-radius: 6px;
}
/*商品详情*/

.zs-details {
	padding: 22px;
	background: #fff;
}

.zs-details .msg {
	position: relative;
	height: 100px;
	overflow: hidden;
}

.zs-details .msg .img {
	position: absolute;
	top: 0px;
	left: 0px;
	width: 100px;
	height: 100px;
	line-height: 100px;
	background: #f5f5f5;
	border-radius: 12px;
	text-align: center;
	overflow: hidden;
}

.zs-details .msg .img img {
	vertical-align: middle;
}

.zs-details .msg .context {
	margin-left: 120px;
	height: 100px;
}

.zs-details .msg .context h2.name {
	margin-bottom: 10px;
	line-height: 40px;
	font-size: 30px;
}
	/*属性*/
.zs-details .msg .context .property {
	color: #999ab2;
	margin-bottom: 12px;
}

.zs-details .msg .context .property span {
	margin-right: 22px;
}
	/*特色*/
.zs-details .msg .context .special .item {
	display: inline-block;
	margin-right: 8px;
	padding: 3px 10px;
	color: #fff;
}

.zs-details .msg .context .special .item.l-green {
	background: #54d591;
}

.zs-details .msg .context .special .item.green {
	background: #22ba22;
}
	/*添加到购物车*/
.zs-details .msg .add {
	position: absolute;
	right: 10px;
	top: 50%;
	margin-top: -20px;
	width: 165px;
	height: 40px;
	line-height: 40px;
	border-radius: 50px;
	background: #5bb533;
	color: #fff;
	font-size: 16px;
	text-indent: 50px;
	cursor: pointer;
}

.zs-details .msg .add:hover {
	background: #41a662;
}

.zs-details .msg .add a {
	position: relative;
	display: block;
	color: #fff;
	z-index: 5;
}

.zs-details .msg .add i.car {
	position: absolute;
	top: 4px;
	left: 4px;
	width: 32px;
	height: 32px;
	background: url(../images/zs-car.png) no-repeat center;
	border-radius: 100%;
}
	/*图片展示*/
.zs-details .picture {
	position: relative;
	margin-top: 30px;
	height: 424px;
	padding: 12px;
	border: 1px #ececec solid;
	background: #f6f6f6;
	overflow: hidden;
}

.zs-details .picture.show-btn:hover i.control {
	display: block;
}

.zs-details .picture i.control {
	position: absolute;
	top: 50%;
	width: 50px;
	margin-top: -25px;
	height: 50px;
